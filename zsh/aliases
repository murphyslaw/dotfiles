#
# Alias
#
alias gitx='gitx --all'
alias ls='ls -lapkh'
alias ..='cd ..'
alias killssh='ps -ef | grep ssh | grep -v grep && killall ssh'
alias diff='diff -bB'
alias mkdir='mkdir -pv'

#
# Vico alias
#
alias vico='open -a /Applications/Vico.app'

